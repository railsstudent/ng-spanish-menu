    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "~4.3.5"
  },
  "volta": {
    "node": "14.17.5",
    "npm": "7.20.6",
    "yarn": "1.22.11"
  },
  "lint-staged": {
    "*.{ts,js,html}": [
